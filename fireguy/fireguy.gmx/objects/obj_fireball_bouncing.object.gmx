        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Fireball movement
if !place_meeting(x,y+1,obj_block) // in the air
{
    grav = 0.5;
    vspd += grav;
}

if place_meeting(x,y+sign(vspd),obj_block) {
    block_ref = instance_place(x,y+sign(vspd),obj_block_moving);
    if (block_ref != noone) scr_fireball_to_block(id, block_ref);
    vspd = -5;
    bounce_count += 1;
}

if place_meeting(x+sign(hspd),y,obj_block) {
    block_ref = instance_place(x+sign(hspd),y,obj_block_moving);
    if (block_ref != noone) scr_fireball_to_block(id, block_ref);
    hspd = -hspd;
    bounce_count += 1;
}

repeat(abs(hspd)) // move horizontally
{
    if (!place_meeting(x+sign(hspd),y,obj_block)) x += sign(hspd);
    else 
    {
        block_ref = instance_place(x+sign(hspd),y,obj_block_moving);
        if (block_ref != noone) scr_fireball_to_block(id, block_ref);
        else 
        {
            hspd = -hspd;
            bounce_count += 1;
            break;
        }
    }
}


repeat(abs(vspd)) // move vertically
{
    if (!place_meeting(x,y+sign(vspd),obj_block)) y += sign(vspd);
    else {
        block_ref = instance_place(x,y+sign(vspd),obj_block_moving);
        if (block_ref != noone) scr_fireball_to_block(id, block_ref);
        else 
        {
        vspd = -5;
        bounce_count += 1;
        break;
        }
    }
}


y += sign(vspd_carry); // add carry
vspd_carry = 0;

if (bounce_count &gt; 3) {
    instance_destroy();
}
</string>
